#include <iostream>
#include <cstring>
#include <string>

using namespace std;

int main()
{


}

void print1(char str[])
{
    for (int i = 0; i < strlen(str); i++)
    {
        cout<< str[i] << endl;
    }
}

void print2(char str[])
{
    int i = 0;
    while (i < strlen(str))
    {
        cout<< str[i] << endl;
        i++;
    }
}

