#include <iostream>
using namespace std;

int main()
{
    int N;
    cout << "I will guess a number from 1 to N. Please enter N: ";
    cin >> N;

    return 0;
}

bool isEven(int n)
{
    return (n % 2) == 0;
}

bool isOdd(int n)
{
    return (n % 2) == 0;
}

int guessNumber(int x)
{
    if(isEven(x))
        return x / 2;
    return 0;
}

